#!/bin/bash
BIND_ADDRESS_QCM="0.0.0.0"
BIND_PORT_QCM="5000"

BIND_ADDRESS_AUTHENTICATION="0.0.0.0"
BIND_PORT_AUTHENTICATION="5001"

BIND_ADDRESS_CORRECTOR="0.0.0.0"
BIND_PORT_CORRECTOR="5002"

ADDRESS_SERVER_QCM="localhost"
PORT_SERVER_QCM=BIND_PORT_QCM

ADDRESS_SERVER_AUTHENTICATION="localhost"
PORT_SERVER_AUTHENTICATION=BIND_PORT_AUTHENTICATION

ADDRESS_SERVER_CORRECTOR="localhost"
PORT_SERVER_CORRECTOR=BIND_PORT_CORRECTOR


export BIND_ADDRESS_QCM
export BIND_PORT_QCM

export BIND_ADDRESS_AUTHENTICATION
export BIND_PORT_AUTHENTICATION

export BIND_ADDRESS_CORRECTOR
export BIND_PORT_CORRECTOR

export ADDRESS_SERVER_QCM
export PORT_SERVER_QCM

export ADDRESS_SERVER_AUTHENTICATION
export PORT_SERVER_AUTHENTICATION

export ADDRESS_SERVER_CORRECTOR
export PORT_SERVER_CORRECTOR
